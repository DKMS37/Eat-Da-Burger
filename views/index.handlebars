<div class="container">
    <div class="jumbotron">
        <h1 id="color-override"></h1>
        <p id="color-override"></p>
        <br>
        <h2 id="color-override"></h2>
        <panel>

        </panel>
    </div>

    <div class="row">
        <!-- Undevoured Burgers -->
        <div class="col-sm-6">
            <div class="panel panel-default">
                <div class="panel-heading" id="lefthead"><i class="fas fa-grin-hearts"></i> <strong>NEW BURGERS</strong> <i class="fas fa-grin-tongue"></i></div>
                <div class="panel-body">
                    <ul>
                        {{#each burgers}}
                        {{#unless devoured}}
                        <span><strong> {{this.id}} - {{this.burger_name}} </strong></span>
                        <button type="button" class="eatburger btn btn-warning" id="eatbutton" data-id="{{this.id}}"
                            data-newDevoured="{{devour}}">Devour It <span id="cutlery" class="fas fa-utensils"></span></button>
                        <hr>
                        {{/unless}}
                        {{/each}}
                    </ul>
                </div>
            </div>
        </div>
        <!-- Devoured Burgers -->
        <div class="col-sm-6">
            <div class="panel panel-default">
                <div class="panel-heading" id="righthead"><i class="far fa-smile-beam"></i> <strong>DEVOURED BURGERS</strong> <i class="far fa-kiss-wink-heart"></i></div>
                <div class="panel-body">
                    <ul>
                        {{#each burgers}}
                        {{#if devoured}}
                        <span><strong> {{this.id}} - {{this.burger_name}} </strong></span>
                        <button type="button" class="trashburger btn btn-danger" id="trashbutton" data-id="{{this.id}}"
                            data-newDevoured="{{devour}}"><span class="fas fa-trash-alt"> finiSHed</span></button>
                        <hr>
                        {{/if}}
                        {{/each}}
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="jumbotron1 text-center">
        <br>
        <h1 class="text-center" id="color-override">CRAVING FOR BURGERS???</h1>
